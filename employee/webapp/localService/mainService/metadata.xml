<?xml version="1.0" encoding="utf-8"?>
<edmx:Edmx Version="4.0" xmlns:edmx="http://docs.oasis-open.org/odata/ns/edmx">
    <edmx:Reference Uri="https://sap.github.io/odata-vocabularies/vocabularies/Common.xml">
        <edmx:Include Alias="Common" Namespace="com.sap.vocabularies.Common.v1"/>
    </edmx:Reference>
    <edmx:Reference Uri="https://oasis-tcs.github.io/odata-vocabularies/vocabularies/Org.OData.Core.V1.xml">
        <edmx:Include Alias="Core" Namespace="Org.OData.Core.V1"/>
    </edmx:Reference>
    <edmx:DataServices>
        <Schema Namespace="TimesheetService" xmlns="http://docs.oasis-open.org/odata/ns/edm">
            <Annotation Term="Core.Links">
                <Collection>
                    <Record>
                        <PropertyValue Property="rel" String="author"/>
                        <PropertyValue Property="href" String="https://cap.cloud.sap"/>
                    </Record>
                </Collection>
            </Annotation>
            <EntityContainer Name="EntityContainer">
                <EntitySet Name="Projects" EntityType="TimesheetService.Projects">
                    <NavigationPropertyBinding Path="assignedEmployees" Target="EmployeeProjects"/>
                </EntitySet>
                <EntitySet Name="Employees" EntityType="TimesheetService.Employees">
                    <NavigationPropertyBinding Path="assignedProjects" Target="EmployeeProjects"/>
                </EntitySet>
                <EntitySet Name="EmployeeProjects" EntityType="TimesheetService.EmployeeProjects">
                    <NavigationPropertyBinding Path="project" Target="Projects"/>
                    <NavigationPropertyBinding Path="employee" Target="Employees"/>
                </EntitySet>
                <ActionImport Name="createProjectWithEmployees" Action="TimesheetService.createProjectWithEmployees"/>
            </EntityContainer>
            <EntityType Name="Projects">
                <Key>
                    <PropertyRef Name="ID"/>
                </Key>
                <Property Name="ID" Type="Edm.Guid" Nullable="false"/>
                <Property Name="projectName" Type="Edm.String" MaxLength="100"/>
                <Property Name="StartDate" Type="Edm.Date"/>
                <Property Name="EndDate" Type="Edm.Date"/>
                <NavigationProperty Name="assignedEmployees" Type="Collection(TimesheetService.EmployeeProjects)" Partner="project">
                    <OnDelete Action="Cascade"/>
                </NavigationProperty>
            </EntityType>
            <EntityType Name="Employees">
                <Key>
                    <PropertyRef Name="ID"/>
                </Key>
                <Property Name="ID" Type="Edm.Guid" Nullable="false"/>
                <Property Name="employeeFirstName" Type="Edm.String" MaxLength="100"/>
                <Property Name="employeeLastName" Type="Edm.String" MaxLength="100"/>
                <Property Name="fullName" Type="Edm.String" MaxLength="100"/>
                <Property Name="gender" Type="Edm.String" MaxLength="10"/>
                <Property Name="email" Type="Edm.String" MaxLength="100"/>
                <Property Name="contactNo" Type="Edm.String" MaxLength="20"/>
                <Property Name="department" Type="Edm.String" MaxLength="50"/>
                <Property Name="joiningDate" Type="Edm.Date"/>
                <Property Name="location" Type="Edm.String" MaxLength="100"/>
                <NavigationProperty Name="assignedProjects" Type="Collection(TimesheetService.EmployeeProjects)" Partner="employee"/>
            </EntityType>
            <EntityType Name="EmployeeProjects">
                <Key>
                    <PropertyRef Name="ID"/>
                </Key>
                <Property Name="ID" Type="Edm.Guid" Nullable="false"/>
                <NavigationProperty Name="project" Type="TimesheetService.Projects" Partner="assignedEmployees">
                    <ReferentialConstraint Property="project_ID" ReferencedProperty="ID"/>
                </NavigationProperty>
                <Property Name="project_ID" Type="Edm.Guid"/>
                <NavigationProperty Name="employee" Type="TimesheetService.Employees" Partner="assignedProjects">
                    <ReferentialConstraint Property="employee_ID" ReferencedProperty="ID"/>
                </NavigationProperty>
                <Property Name="employee_ID" Type="Edm.Guid"/>
                <Property Name="assignedDate" Type="Edm.Date"/>
            </EntityType>
            <ComplexType Name="createProjectInput">
                <Property Name="projectName" Type="Edm.String"/>
                <Property Name="startDate" Type="Edm.Date"/>
                <Property Name="endDate" Type="Edm.Date"/>
                <Property Name="employeeIDs" Type="Collection(Edm.Guid)" Nullable="true"/>
            </ComplexType>
            <Action Name="createProjectWithEmployees" IsBound="false">
                <Parameter Name="input" Type="TimesheetService.createProjectInput"/>
                <ReturnType Type="Collection(Edm.String)" Nullable="true"/>
            </Action>
            <Annotations Target="TimesheetService.Projects/ID">
                <Annotation Term="Core.ComputedDefaultValue" Bool="true"/>
            </Annotations>
            <Annotations Target="TimesheetService.Employees/ID">
                <Annotation Term="Core.ComputedDefaultValue" Bool="true"/>
            </Annotations>
            <Annotations Target="TimesheetService.EmployeeProjects/ID">
                <Annotation Term="Core.ComputedDefaultValue" Bool="true"/>
            </Annotations>
        </Schema>
    </edmx:DataServices>
</edmx:Edmx>